    <div class="wrapper">
        <section id="news">
            <p class="super-headline">Making waves since 2004</p>
            <h2>In the News</h2>
            <div class="row">
                {% assign news = collections.news | reverse %}
                {% for n in news limit:3 %}
                <article> 
                    <h3>{{ n.data.title }}</h3>
                    <img src="{{ n.data.picture }}" alt="{{ n.data.picture-alt }}"> 
                    <p>{{ n.data.excerpt }}</p>
                    <a href="{{ n.url }}" class="button">Read more</a>
                </article>
                {% endfor%}
            </div>
        </section>
    </div>
